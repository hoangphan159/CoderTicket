<!DOCTYPE html>
<html>
<head>
  <title>CsTemplate</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav class="navbar navbar-dark bg-inverse">
    <a class="navbar-brand" href="<%= root_path %>">Coder Ticket</a>
    <ul class="nav navbar-nav">
      <% if user_signed_in? %>
      <li class="nav-item">
        <%= link_to "New Event", new_event_path, class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "New Venue", new_venue_path, class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "My Events", mine_events_path, class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "Log Out", destroy_user_session_path, class: "nav-link", method: :delete %>
      </li>
      <% else %>
      <li class="nav-item">
        <%= link_to "Log In", new_user_session_path, class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "Sign Up", new_user_registration_path, class: "nav-link" %>
      </li>
      <% end %>
    </ul>
  </nav>

<%= flash_messages %>
<%= yield %>

</body>
</html>
